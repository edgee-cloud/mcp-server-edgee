{
  "name": "@edgee/mcp-server-edgee",
  "version": "0.1.3",
  "description": "MCP server for interacting with Edgee API",
  "keywords": [
    "mcp",
    "edgee",
    "edge",
    "wasm"
  ],
  "license": "Apache-2.0",
  "author": "Edgee Cloud (https://edgee.cloud)",
  "homepage": "https://github.com/edgee-cloud/mcp-server-edgee",
  "bugs": "https://github.com/edgee-cloud/mcp-server-edgee/issues",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "access": "public",
  "main": "./dist/index.js",
  "bin": {
    "edgee": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc -p tsconfig.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "typescript": "^5.9.2"
  }
}
